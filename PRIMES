def prime(a):
    if a>1:
        for i in range(2,a):
            if a%i==0:
                return False
        return True
for i in range(200,1000):
    x=str(i)
    a=int(x[0])
    b=int(x[1])
    c=int(x[2])
    if(prime(i) and prime(a) and prime(b) and prime(c) and prime(a+b+c)):
        print(i)
